{
  "source": {
    "includePattern": ".+\\.js$"
  },
  "plugins": ["node_modules/jsdoc-babel"],
  "babel": {
    // Just transform the things that jsdoc can't parse
    "plugins": [
      "transform-class-properties",
      "transform-react-jsx",
      "transform-object-rest-spread"
    ],
    "babelrc": false
  }
}
